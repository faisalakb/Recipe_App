<h1>New InventoryFood</h1>

<%= form_for @inventory_food do |f| %>
  <div class="field">
    <%= f.label :quantity %>
    <%= f.number_field :quantity %>
  </div>

  <div class="field">
    <%= f.label :inventory_id %>
    <%= f.number_field :inventory_id %>
  </div>

  <div class="field">
    <%= f.label :food_id %>
    <%= f.number_field :food_id %>
  </div>

  <div class="actions">
    <%= f.submit 'Create InventoryFood' %>
  </div>
<% end %>
